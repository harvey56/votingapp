{
  "name": "react-redux-mongo-voting-app",
  "version": "1.0.0",
  "description": "Voting app made from scratch with React, Redux, Express, MongoDB",
  "main": "dist/server/index.js",
  "scripts": {
    "clean": "rm -rf dist && mkdir -p dist/client/ && mkdir -p dist/server/",
    "build-server": "babel ./server -d dist/server",
    "create-html": "cp ./server/index.html ./public/stylesheets/main.css ./dist/client/",
    "cp-public": "cp -R ./public dist",
    "prebuild": "npm-run-all clean build-server create-html cp-public",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "start": "node dist/server/index.js",
    "dev": "NODE_ENV=development nodemon --watch server --exec babel-node -- server/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "harvey56",
  "repository": {
    "type": "git",
    "url": "https://github.com/harvey56/votingapp.git"
  },
  "license": "ISC",
  "dependencies": {
    "assert": "^1.4.1",
    "axios": "^0.15.3",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "body-parser": "^1.17.2",
    "chart.js": "^2.5.0",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "cross-env": "^5.0.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "jsonwebtoken": "^7.4.0",
    "mongodb": "^2.2.26",
    "mongoose": "^4.9.7",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-chartjs": "^0.8.0",
    "react-chartjs-2": "^2.1.0",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-router": "^3.0.5",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^5.0.1",
    "shortid": "^2.2.8",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.8",
    "validator": "^6.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.26.4",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "react-hot-loader": "^1.3.1",
    "react-loader": "^2.4.1",
    "webpack": "^2.5.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  }
}
